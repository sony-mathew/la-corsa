<% if @learn_tab and (@current_sub_tab or @completed_sub_tab or @dropped_sub_tab) %>
			<!-- Sub Navigation Bar  Start-->		
			<ul class="nav nav-tabs sub-nav-bar" role="tablist">
				<% base_url = '/courses/enrolled' %>
			  <li class="<%= "active" if @current_sub_tab %>"><a href="<%= base_url %>/current">Current</a></li>
			  <li class="<%= "active" if @completed_sub_tab %>"><a href="<%= base_url %>/completed">Completed</a></li>
			  <li class="<%= "active" if @dropped_sub_tab %>"><a href="<%= base_url %>/dropped">Dropped</a></li>
			</ul>
			<!-- Sub Navigation Bar end -->
<% elsif @teach_tab %>
			<!-- Sub Navigation Bar  Start-->		
			<ul class="nav nav-tabs sub-nav-bar" role="tablist">
				<li class="dropdown <%= "active" if @study_materials_sub_tab %>">
			        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
			          	Study Materials
			          	<span class="caret"></span>
			        </a>
			        <ul class="dropdown-menu" role="menu">
			            <li><%= link_to "My Materials", study_materials_path %></li>
			            <li><%= link_to "All Materials", "/study_materials/filter/all" %></li>
			            <li><%= link_to "Add New", new_study_material_path %></li>
			        </ul>
			    </li>

			  	<li class="<%= "active" if @courses_sub_tab %>">
			  		<a href="#" class="dropdown-toggle" data-toggle="dropdown">
			          	Courses
			          	<span class="caret"></span>
			        </a>
			        <ul class="dropdown-menu" role="menu">
			            <li><%= link_to "My Courses", courses_path %></li>
			            <li><%= link_to "All Courses", "/courses/filter/all" %></li>
			            <li><%= link_to "Add New", new_course_path %></li>
			        </ul>
			  	</li>
			  	<li class="<%= "active" if @students_sub_tab %>"><a href="/users/<%= current_user.id %>/students">Students</a></li>
			</ul>
			<!-- Sub Navigation Bar end -->
<% end %>

<br/ >